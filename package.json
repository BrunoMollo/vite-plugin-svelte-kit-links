{
  "name": "vite-plugin-svelte-kit-links",
  "version": "0.0.1",
  "description": "",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "./dist/*"
  ],
  "scripts": {
    "prepare": "husky install",
    "prepublishOnly": "pnpm run build",
    "build": "tsup",
    "build:dev": "pnpm run build --watch",
    "lint": "eslint . --ext js,ts,cjs,mjs",
    "lint:fix": "pnpm run lint --fix",
    "format": "prettier './**/*.{js,ts,cjs,mjs}'",
    "format:check": "pnpm run format --check",
    "format:fix": "pnpm run format --write"
  },
  "lint-staged": {
    "./**/*.{js,ts,cjs,mjs}": [
      "pnpm run lint:fix"
    ],
    "./**/*.{js,ts,cjs,mjs,json}": [
      "pnpm run format:fix"
    ]
  },
  "keywords": [
    "PurgeCSS",
    "Vite",
    "Vite Plugin"
  ],
  "author": {
    "name": "Joe Stanley",
    "url": "https://joeknows.dev"
  },
  "license": "MIT",
  "peerDependencies": {
    "vite": "^3 || ^4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.38.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "prettier-cli": "^0.1.0",
    "rollup": "^3.20.2",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vite": "^4.2.1"
  },
  "dependencies": {
    "@types/node": "^20.11.28",
    "purgecss": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "publishConfig": {
    "access": "public"
  }
}
